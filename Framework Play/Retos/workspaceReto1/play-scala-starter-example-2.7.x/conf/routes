# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

#Calculadora Controller
POST	/calculadora				controllers.CalculadoraController.calcular

#Persona Controller
GET		/buscarId/:id				controllers.PersonaController.buscarId(id: Int)

GET		/buscarNombre/:nombre		controllers.PersonaController.buscarNombre(nombre: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(file)
